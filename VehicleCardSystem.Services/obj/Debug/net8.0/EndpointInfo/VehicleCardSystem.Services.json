  "components": {
    "schemas": {
      "Vehicle": {
        "required": [
          "color",
          "modelYear",
          "plate",
          "vehicletypeId"
        ],
        "type": "object",
        "properties": {
          "vehicleId": {
            "type": "string",
            "format": "uuid"
          },
          "plate": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "modelYear": {
            "maximum": 2100,
            "minimum": 1900,
            "type": "integer",
            "format": "int32"
          },
          "color": {
            "minLength": 1,
            "type": "string"
          },
          "vehicletypeId": {
            "type": "string",
            "format": "uuid"
          },
          "vehicleTypes": {
